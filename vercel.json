{
    "version": 2,
    "builds": [
      {
        "src": "Listify/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Listify/wsgi.py"
      }
    ],
    "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate"
  }